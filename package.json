{
  "name": "my-studio-api",
  "version": "1.0.1",
  "main": "src/handler.js",
  "license": "MIT",
  "scripts": {
    "deploy:env": "sls deploy --stage $NODE_ENV",
    "deploy": "export NODE_ENV=dev && yarn deploy:env",
    "deploy:production": "export NODE_ENV=production && yarn deploy:env",
    "deploy:staging": "serverless deploy --stage staging --region us-west-2",
    "lint": "eslint ./functions",
    "precommit": "lint-staged",
    "serve": "serverless offline start --port 3000",
    "serve:admin": "export AUTHORIZER='{\"claims\": {\"cognito:groups\": \"admin, namaspa\"}}'; serverless offline start --port 3000",
    "serve:teacher": "export AUTHORIZER='{\"claims\": {\"cognito:groups\": \"teacher, namaspa\"}}'; serverless offline start --port 3000",
    "tail:hello": "serverless logs --function hello --tail",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "serverless webpack",
    "watch:hello": "serverless invoke local --watch --function hello --path fixtures/event.json",
    "watch:warm": "serverless invoke local --watch --function hello --path fixtures/scheduled.json"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^1.0.0",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "serverless": "^1.32.0",
    "serverless-iam-roles-per-function": "^1.0.4",
    "serverless-offline": "^3.25.10",
    "serverless-webpack": "^5.2.0",
    "webpack": "^4.17.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "aws-sdk": "^2.382.0",
    "cheerio": "^1.0.0-rc.2",
    "form-data": "^2.3.3",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "mysql2": "^1.6.1",
    "object-dig": "^0.1.3",
    "puppeteer": "^1.11.0",
    "sequelize": "^4.39.0",
    "serverless-dynamodb-local": "0.2.30",
    "serverless-offline-sqs": "^1.6.1",
    "serverless-plugin-split-stacks": "^1.8.0"
  },
  "lint-staged": {
    "functions/**/*.js": [
      "yarn lint -- --fix",
      "prettier --write",
      "git add"
    ]
  }
}
